@model IEnumerable<SampleDockerApp.Models.Employee>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Employees</title>
</head>
<body>
    <h1>Employees</h1>
    <ul>
        @foreach (var employee in Model)
        {
            <li>@employee.Name - @employee.Position</li>
        }
    </ul>
</body>
</html>
